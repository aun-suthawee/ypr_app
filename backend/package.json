{
  "name": "ypr-backend",
  "version": "1.0.0",
  "description": "YPR Dashboard Backend API",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "init-db": "node src/database/init.js",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "echo \"ESLint disabled for CommonJS project\"",
    "setup": "npm install && npm run init-db",
    "clean": "rmdir /s /q node_modules 2>nul || rm -rf node_modules",
    "fresh-install": "npm run clean && npm install",
    "update-deps": "npm update",
    "audit": "npm audit",
    "audit-fix": "npm audit fix"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "axios": "^1.10.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.0",
    "react-hook-form": "^7.59.0",
    "uuid": "^10.0.0",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  },
  "keywords": [
    "ypr",
    "dashboard",
    "backend",
    "api"
  ],
  "author": "YPR Team",
  "license": "MIT"
}
